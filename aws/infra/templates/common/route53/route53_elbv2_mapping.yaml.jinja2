HostedZoneId: {{ inputs.fargate_route_53.hosted_zone_id }}
ChangeBatch:
  Comment: "{{ inputs.fargate_route_53.change_batch.comment }}"
  {% for change in inputs.fargate_route_53.change_batch.changes -%}
  Changes:
  - Action: {{ change.action }}
    ResourceRecordSet:
      Name: {{ change.resource_record_set.name }}
      Type: {{ change.resource_record_set.type }}
      TTL: {{ change.resource_record_set.ttl }}
      {% for resource_record in change.resource_record_set.resource_records -%}
        ResourceRecords:
        - Value: {{ resource_record.value }}
      {% endfor -%}
  {% endfor -%}
